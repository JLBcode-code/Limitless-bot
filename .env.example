# 示例 .env 配置文件（请复制为 .env 并在本地填写）
RPC_URL=https://your.rpc.node.endpoint  # RPC 节点 URL（例如 Alchemy/Infura 或自建节点）
CHAIN_ID=8453
PRICE_ORACLE_ID=60
#这里参数是时间市场比如Minutes 15就是15分钟场，其他选项：Minutes 5，Hourly，Daily
FREQUENCY=Minutes 15
#开盘后入场时间限制，这里填6也就是6分钟后判断入场机会
MIN_MARKET_AGE_MIN=6
#每盘将要截止时间跳过买入
MAX_MARKET_AGE_MIN=4
# 你的钱包私钥（逗号分隔，示例：0xabc...,0xdef...）
PRIVATE_KEYS=

POLL_INTERVAL_MS=4000
# 每次购买的金额
BUY_AMOUNT_USDC=5
# 止盈百分比
TARGET_PROFIT_PCT=20
SLIPPAGE_BPS=100
MAX_GAS_ETH=0.018
GAS_PRICE_GWEI=0.01
CONFIRMATIONS=1

# 策略选择默认即可
# STRATEGY_MODE=dominant -> 买入 >= TRIGGER_PCT
# STRATEGY_MODE=opposite -> 如果一方的价格在 TRIGGER_BAND 范围内（即 TRIGGER_PCT 的阈值），则买入另一方的价格。
STRATEGY_MODE=dominant
TRIGGER_PCT=60
TRIGGER_BAND=5
